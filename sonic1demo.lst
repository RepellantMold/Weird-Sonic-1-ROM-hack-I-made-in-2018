00001568 3418                               move.w  (a0)+,d2                ; get number of patterns
0000156A 6A00                               bpl.s   @0                      ; are we in Mode 0?
0000156C 47EB 000A                          lea     $A(a3),a3               ; if not, use Mode 1
00001570 E74A                       @0      lsl.w   #3,d2
00001572 3A42                               movea.w d2,a5
00001574 7607                               moveq   #7,d3
00001576 7400                               moveq   #0,d2
00001578 7800                               moveq   #0,d4
0000157A 6100 0000                          bsr.w   NemDec4
0000157E 1A18                               move.b  (a0)+,d5                ; get first byte of compressed data
00001580 E145                               asl.w   #8,d5                   ; shift up by a byte
00001582 1A18                               move.b  (a0)+,d5                ; get second byte of compressed data
00001584 3C3C 0010                          move.w  #$10,d6                 ; set initial shift value
00001588 6100                               bsr.s   NemDec2
