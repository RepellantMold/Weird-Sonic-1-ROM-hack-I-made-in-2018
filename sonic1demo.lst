00144400 =0000002A                  obj_control  = 		$2A ; 0 for normal, 1 for hanging or for resting on a flipper, $81 for going through CNZ/OOZ/MTZ tubes or stopped in CNZ cages or stoppers or flying if Tails
00144400 =0000002B                  status_secondary  = 	$2B
00144400 =0000002C                  flips_remaining  = 	$2C ; number of flip revolutions remaining
00144400 =0000002D                  flip_speed  = 		$2D ; number of flip revolutions per frame / 256
00144400 =0000002E                  move_lock  = 		$2E ; and $2F ; horizontal control lock, counts down to 0
