00144400 =0000001C                  anim  = 			$1C
00144400 =0000001D                  next_anim  = 		$1D
00144400 =0000001E                  anim_frame_duration  = 	$1E
00144400 =00000022                  status  = 		$22 ; note: exact meaning depends on the object... for sonic/tails: bit 0: leftfacing. bit 1: inair. bit 2: spinning. bit 3: onobject. bit 4: rolljumping. bit 5: pushing. bit 6: underwater.
00144400 =00000024                  routine  = 		$24
00144400 =00000025                  routine_secondary  = 	$25
00144400 =00000026                  angle  = 			$26 ; angle about the z = 0 axis (360 degrees  =  256)
00144400                            ; ---------------------------------------------------------------------------
00144400                            ; conventions followed by many objects but NOT sonic/tails:
00144400 =00000020                  collision_flags  = 	$20
00144400 =00000021                  collision_property  = 	$21
00144400 =00000023                  respawn_index  = 		$23
00144400 =00000028                  subtype  = 		$28
00144400                            ; ---------------------------------------------------------------------------
