00001590                            ; ---------------------------------------------------------------------------
00001590                            ; Part of the Nemesis decompressor, processes the actual compressed data
00001590                            ; ---------------------------------------------------------------------------
00001590                             
00001590                            NemDec2:
00001590 3E06                               move.w  d6,d7
00001592 5147                               subq.w  #8,d7                   ; get shift value
00001594 3205                               move.w  d5,d1
00001596 EE69                               lsr.w   d7,d1                   ; shift so that high bit of the code is in bit position 7
00001598 0C01 00FC                          cmpi.b  #%11111100,d1           ; are the high 6 bits set?
0000159C 6400                               bcc.s   NemDec_InlineData       ; if they are, it signifies inline data
0000159E 0241 00FF                          andi.w  #$FF,d1
000015A2 D241                               add.w   d1,d1
000015A4 9C31 1000                          sub.b   (a1,d1.w),d6            ; ~~ subtract from shift value so that the next code is read next time around
000015A8 0C46 0009                          cmpi.w  #9,d6                   ; does a new byte need to be read?
000015AC 6400                               bcc.s   @0                      ; if not, branch
000015AE 5046                               addq.w  #8,d6
000015B0 E145                               asl.w   #8,d5
000015B2 1A18                               move.b  (a0)+,d5                ; read next byte
000015B4 1231 1001                  @0      move.b  1(a1,d1.w),d1
000015B8 3001                               move.w  d1,d0
000015BA 0241 000F                          andi.w  #$F,d1                  ; get palette index for pixel
000015BE 0240 00F0                          andi.w  #$F0,d0
