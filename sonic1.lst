00144400                            ; Object Status Table offsets (for everything between Object_RAM and Primary_Collision)
00144400                            ; ---------------------------------------------------------------------------
00144400 =00000001                  render_flags =		  1 ; bitfield ; bit 7 = onscreen flag, bit 0 = x mirror, bit 1 = y mirror, bit 2 = coordinate system
00144400 =00000002                  art_tile =		  2 ; and 3 ; start of sprite's art
00144400 =00000004                  mappings =		  4 ; and 5 and 6 and 7
00144400 =00000008                  x_pos =			  8 ; and 9 ... some objects use $A and $B as well when extra precision is r=ired (see ObjectMove) ... for screen-space objects this is called x_pixel instead
00144400 =0000000C                  y_pos =			 $C ; and $D ... some objects use $E and $F as well when extra precision is r=ired ... screen-space objects use y_pixel instead
00144400 =00000018                  priority =		$18 ; 0 = front
00144400 =00000019                  width_pixels =		$19
00144400 =0000001A                  mapping_frame =		$1A
00144400                            ; ---------------------------------------------------------------------------
00144400                            ; conventions followed by most objects:
00144400 =00000010                  x_vel  = 			$10 ; and $11 ; horizontal velocity
00144400 =00000012                  y_vel  = 			$12 ; and $13 ; vertical velocity
00144400 =00000016                  y_radius  = 		$16 ; collision width / 2
00144400 =00000017                  x_radius  = 		$17 ; collision height / 2
