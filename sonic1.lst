000015D8 5D46                               subq.w  #6,d6                   ; 6 bits needed to signal inline data
000015DA 0C46 0009                          cmpi.w  #9,d6
000015DE 6400                               bcc.s   @0
000015E0 5046                               addq.w  #8,d6
000015E2 E145                               asl.w   #8,d5
000015E4 1A18                               move.b  (a0)+,d5
000015E6 5F46                       @0      subq.w  #7,d6                   ; and 7 bits needed for the inline data itself
000015E8 3205                               move.w  d5,d1
000015EA EC69                               lsr.w   d6,d1                   ; shift so that low bit of the code is in bit position 0
000015EC 3001                               move.w  d1,d0
000015EE 0241 000F                          andi.w  #$F,d1                  ; get palette index for pixel
000015F2 0240 0070                          andi.w  #$70,d0                 ; high nybble is repeat count for pixel
000015F6 0C46 0009                          cmpi.w  #9,d6
000015FA 64C6                               bcc.s   NemDec_GetRepeatCount
000015FC 5046                               addq.w  #8,d6
000015FE E145                               asl.w   #8,d5
00001600 1A18                               move.b  (a0)+,d5
